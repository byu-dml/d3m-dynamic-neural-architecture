{
    "__init__": {
        "activation_name": "relu",
        "use_batch_norm": true,
        "use_skip": false,

        "hidden_state_size": 2,
        "lstm_n_layers": 2,
        "dropout": 0.5,
        "reduction_name": "mean",

        "output_n_hidden_layers": 1,
        "output_hidden_layer_size": 4
    },
    "fit": {
        "n_epochs": 1,
        "learning_rate": 0.0001,
        "batch_size": 32,
        "drop_last": true,
        "validation_ratio": 0.1,
        "patience": 20
    },
    "predict_regression": {
        "batch_size": 32
    },
    "predict_rank": {
        "batch_size": 32
    },
    "predict_subset": {
        "batch_size": 32
    }
}
