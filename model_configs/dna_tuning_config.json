{
    "population_size": 2,
    "n_generations": 5,
    "output_path": "./tuning_results_test",
    "output": true,
    "attributes": {
        "hidden_layer_size": {
            "type": "int",
            "min": 1,
            "max": 512,
            "step": 1
        },
        "n_hidden_layers": {
            "type": "int",
            "min": 1,
            "max": 10,
            "step": 1
        },
        "use_batch_norm": {
            "type": "bool"
        },
        "use_skip": {
            "type": "bool" 
        },
        "dropout": {
            "type": "float",
            "min": 0,
            "max": 1,
            "step": 0.1
        },
        "learning_rate": {
            "type": "float",
            "min": 0.0001,
            "max": 0.001,
            "step": 0.0001
        },
        "patience": {
            "type": "int",
            "min": 0,
            "max": 10,
            "step": 1
        }
    }
}
